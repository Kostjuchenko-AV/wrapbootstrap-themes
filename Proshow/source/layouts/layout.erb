<body>
<div id="main_wrapper">
<header>
	<div class="container-fluid bg-transparent">
		<div class="row-fluid" id="top-bar">
			<div class="span4">
				<div id="logo">
					<!-- <h1 class="brand"><i class="icon-screenshot"></i> logo here</h1> -->
					<img src="img/logo.png" alt="">
				</div>
			</div>
			<div class="span8">
				<ul id="menu-bar" class="pull-right">
					<li><a href="#latest-news">News</a></li>
					<li><a href="#features-start">Features</a></li>
					<li><a href="#guide-tour">Guide</a></li>
					<li><a href="#screenshots">Screenshots</a></li>
					<li><a href="#pricing">Pricing</a></li>
					<li><a href="#faq">Faq</a></li>
					<li><a href="#contact-modal" data-toggle="modal">Contact</a></li> 
				</ul>
			</div>
		</div>
	</div>
</header>	

<!--content goes here-->
<%=yield%>
<!--/content-->

<div class="container-fluid">
	<div id="contact-modal" class="modal hide fade" role="dialog" tabindex="-1">
		<button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
		<form>
	      <h3 id="contact">
	      	<a href="javascript:void(0)"><i class="icon-twitter"></i></a>
	      	<a href="javascript:void(0)"><i class="icon-facebook"></i></a>
	      </h3>
	      <h4>Or, Leave Message for Us</h4>
	      <div>
	        <label>First Name</label>
	        <input type="text" class="span3" placeholder="Your First Name">
	        <label>Last Name</label>
	        <input type="text" class="span3" placeholder="Your Last Name">
	        <label>Email Address</label>
	        <input type="text" class="span3" placeholder="Your email address">
	      </div>
	      <div>
	        <label>Message</label>
	        <textarea class="span6 google-keep-warning" name="message" id="message" rows="6"></textarea>
	      </div>
	      <br>
	      <button type="submit" class="btn btn-info"><i class="icon-edit"></i> Send Message</button>
	  </form>
	</div>
</div>

